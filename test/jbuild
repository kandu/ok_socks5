(jbuild_version 1)

(executable
 ((name test)
  (flags (:standard -safe-string))
  (modules (test))
  (libraries (ok_socks5 oUnit))
  (modes (byte))
  ))

(executable
 ((name demo)
  (public_name demo)
  (flags (:standard -safe-string))
  (modules (demo))
  (libraries (ok_socks5 core_kernel lwt.unix ppx_deriving.runtime))
  (preprocess (pps (lwt.ppx -no-sequence ppx_ok_lwt ppx_ok_monad ppx_deriving.std)))
  ))

(alias
 ((name runtest)
  (deps (test.exe))
  (action (run ${<}))))

